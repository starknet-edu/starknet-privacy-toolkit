{
  "name": "tongo-donation-demo",
  "version": "1.0.0",
  "description": "Private donation demo using Tongo Cash on Starknet",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:web": "vite build",
    "dev": "bun run src/demo.ts",
    "dev:web": "vite",
    "serve": "bunx http-server . -p 8080",
    "demo": "bun run build && bun run dist/demo.js",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "starknet",
