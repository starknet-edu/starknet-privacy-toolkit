  "dependencies": {
    "@fatsolutions/tongo-sdk": "^1.3.0",
    "dotenv": "^17.2.3",
    "get-starknet": "^3.3.3",
    "get-starknet-core": "^3.3.5",
    "starknet": "^8.9.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  }
