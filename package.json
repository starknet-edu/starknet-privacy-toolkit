{
  "name": "tongo-donation-demo",
  "version": "1.0.0",
  "description": "Private donation demo using Tongo Cash on Starknet",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:web": "vite build",
    "dev": "bun run src/demo.ts",
    "dev:web": "vite",
    "api": "bun run api/server.ts",
    "serve": "bunx http-server . -p 8080",
    "demo": "bun run build && bun run dist/demo.js",
    "tongo:init": "bun run scripts/tongo-init.ts",
    "preflight:tx": "bun run src/tx-preflight.ts",
    "garaga:smoke": "bun run scripts/garaga-smoke.ts",
    "garaga:verify": "bun run scripts/garaga-verify.ts",
    "check:health": "bun run scripts/health-check.ts",
    "template:quickstart": "bun run template/quickstart.ts",
    "check": "bun run type-check",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "starknet",
    "tongo",
    "privacy",
    "donations",
    "zk"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fatsolutions/tongo-sdk": "^1.3.1",
    "circomlibjs": "^0.1.7",
    "dotenv": "^17.2.3",
    "get-starknet": "^3.3.3",
    "get-starknet-core": "^3.3.5",
    "starknet": "^8.9.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  }
}
